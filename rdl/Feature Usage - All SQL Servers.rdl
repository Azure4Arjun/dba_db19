        <Field Name="NotificationSubscriptions">
          <DataField>NotificationSubscriptions</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FTTables">
          <DataField>FTTables</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="PartitionSchemes">
          <DataField>PartitionSchemes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ReportsGenerated">
          <DataField>ReportsGenerated</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Queues">
          <DataField>Queues</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TextInRow">
          <DataField>TextInRow</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     i.srv_id, i.inst_id, m.name, i.version, i.major, r.run_id, r.datein,
                          (SELECT     COUNT(*)
                            FROM          c_database d
                            WHERE      i.run_id = d.run_id AND i.srv_id = d.srv_Id AND i.inst_id = d.inst_id AND oFullTextEnabled &gt; 0) AS FullTextEnabled,
                          (SELECT     COUNT(*)
                            FROM          c_database d
                            WHERE      i.run_id = d.run_id AND i.srv_id = d.srv_Id AND i.inst_id = d.inst_id AND oPublished &gt; 0) AS Published,
                          (SELECT     COUNT(*)
                            FROM          c_database d
                            WHERE      i.run_id = d.run_id AND i.srv_id = d.srv_Id AND i.inst_id = d.inst_id AND oSubscribed &gt; 0) AS Subscribed,
                          (SELECT     COUNT(*)
                            FROM          c_database d
                            WHERE      i.run_id = d.run_id AND i.srv_id = d.srv_Id AND i.inst_id = d.inst_id AND oMergePublished &gt; 0) AS MergePublished,
                          (SELECT     COUNT(*)
                            FROM          c_database d
                            WHERE      i.run_id = d.run_id AND i.srv_id = d.srv_Id AND i.inst_id = d.inst_id AND oMergeSubscribed &gt; 0) AS MergeSubscribed,
                          (SELECT     COUNT(*)
                            FROM          c_database2 d
                            WHERE      i.run_id = d.run_id AND i.srv_id = d.srv_Id AND i.inst_id = d.inst_id AND oDistributor &gt; 0) AS Distributor,
                          (SELECT     COUNT(*)
                            FROM          c_database2 d
                            WHERE      i.run_id = d.run_id AND i.srv_id = d.srv_Id AND i.inst_id = d.inst_id AND oBrokerEnabled &gt; 0) AS BrokerEnabled,
                          (SELECT     COUNT(*)
                            FROM          c_database2 d
                            WHERE      i.run_id = d.run_id AND i.srv_id = d.srv_Id AND i.inst_id = d.inst_id AND oReadCommittedSnapshotOn &gt; 0) 
                      AS ReadCommittedSnapshotOn,
                          (SELECT     SUM(qIndexedViews)
                            FROM          c_database2 d
                            WHERE      i.run_id = d.run_id AND i.srv_id = d.srv_Id AND i.inst_id = d.inst_id AND qIndexedViews &gt; 0) AS IndexedViews,
                          (SELECT     COUNT(*)
                            FROM          c_database2 d
                            WHERE      i.run_id = d.run_id AND i.srv_id = d.srv_Id AND i.inst_id = d.inst_id AND mirroringWitness &gt; 0) AS Mirroring,
                          (SELECT     COUNT(*)
                            FROM          c_database2 d
                            WHERE      i.run_id = d.run_id AND i.srv_id = d.srv_Id AND i.inst_id = d.inst_id AND IsRosetta &gt; 0) AS RS,
                          (SELECT     SUM(qObjects)
                            FROM          c_assemblies a
                            WHERE      a.run_id = i.run_id AND a.srv_id = i.srv_id AND a.inst_id = i.inst_id) AS Assemblies,
                          (SELECT     SUM(qObjects)
                            FROM          c_backups a
                            WHERE      a.run_id = i.run_id AND a.srv_id = i.srv_id AND a.inst_id = i.inst_id) AS Backups,
                          (SELECT     SUM(qObjects)
                            FROM          c_event_notifications a
                            WHERE      a.run_id = i.run_id AND a.srv_id = i.srv_id AND a.inst_id = i.inst_id) AS Notifications,
                          (SELECT     SUM(qObjects)
                            FROM          c_notification_subscriptions a
                            WHERE      a.run_id = i.run_id AND a.srv_id = i.srv_id AND a.inst_id = i.inst_id) AS NotificationSubscriptions,
                          (SELECT     SUM(TABLEs)
                            FROM          c_ft a
                            WHERE      a.run_id = i.run_id AND a.srv_id = i.srv_id AND a.inst_id = i.inst_id AND TABLEs &gt; 0) AS FTTables,
                          (SELECT     COUNT(*)
                            FROM          c_partition_scheme a
                            WHERE      a.run_id = i.run_id AND a.srv_id = i.srv_id AND a.inst_id = i.inst_id) AS PartitionSchemes,
                          (SELECT     SUM(qObjects)
                            FROM          c_rs a
                            WHERE      a.run_id = i.run_id AND a.srv_id = i.srv_id AND a.inst_id = i.inst_id AND qObjects &gt; 0) AS ReportsGenerated,
                          (SELECT     SUM(Queues)
                            FROM          c_sb a
                            WHERE      a.run_id = i.run_id AND a.srv_id = i.srv_id AND a.inst_id = i.inst_id AND Queues &gt; 0) AS Queues,
                          (SELECT     SUM(qObjects)
                            FROM          c_TableTextInRowLimit a
                            WHERE      a.run_id = i.run_id AND a.srv_id = i.srv_id AND a.inst_id = i.inst_id AND Limit &gt; 0) AS TextInRow
FROM         dbo.c_instance i INNER JOIN
                      dbo.mgr_instance m ON i.srv_id = m.srv_id AND i.inst_id = m.inst_id INNER JOIN
                      dbo.mgr_run r ON i.run_id = r.run_id
ORDER BY m.name</CommandText>
      </Query>
    </DataSet>
  </DataSets>
