    <DataSet Name="SizeDynamics">
      <Fields>
        <Field Name="run_id">
          <DataField>run_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="datein">
          <DataField>datein</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="dbSize">
          <DataField>dbSize</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="SpaceAvailable">
          <DataField>SpaceAvailable</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="txSize">
          <DataField>txSize</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="txSpaceAvailable">
          <DataField>txSpaceAvailable</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     r.run_id, r.datein, d.dbSize, CASE WHEN SpaceAvailable &gt; 0 THEN SpaceAvailable ELSE 0 END AS SpaceAvailable, d.txSize, d.txSpaceAvailable
FROM         dbo.hd_database d INNER JOIN
                      dbo.mgr_run r ON d.run_id = r.run_id
WHERE     (d.srv_id = @srv_id) AND (d.inst_id = @inst_id) AND (d.dbid = @dbid) AND (r.datein &gt;= DATEADD(month, - 1, CONVERT(nvarchar(32), GETDATE(), 101)))
ORDER BY r.run_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>=Parameters!srv_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inst_id">
            <Value>=Parameters!inst_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dbid">
            <Value>=Parameters!dbId.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
