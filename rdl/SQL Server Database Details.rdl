        </Field>
        <Field Name="AutoGrow">
          <DataField>AutoGrow</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     d.dbid, d.name, d.CompatibilityLevel, d.CreateDate, d.DataSpaceUsage, d.IndexSpaceUsage, d.dbSize, d.SpaceAvailable, d.txSize, 
                      d.txSpaceAvailable, d.txLastBackup, m.name AS RecoveryModel, d.oAutoClose, d.oAutoShrink, d.oAutoCREATEStat, d.oAutoUpdateStat, d.oOffline, 
                      d.oReadOnly, d.oSelectIntoBulkCopy, d.oTruncateLogOnCheckpoint, d.oPublished, d.oSubscribed, d.oMergePublished, d.oMergeSubscribed, 
                      d.oFullTextEnabled, d.oDbChaining, p.name AS PageVerify, a.name AS UserAccess, r.run_id, r.datein,
                          (SELECT     COUNT(*)
                            FROM          c_dbfile f
                            WHERE      f.srv_id = d.srv_id AND f.inst_id = d.inst_id AND f.dbid = d.dbid AND Growth &gt; 0) AS AutoGrow
FROM         dbo.c_database d INNER JOIN
                      dbo.d_RecoveryModel m ON d.oRecoveryModel = m.RecoveryModel INNER JOIN
                      dbo.d_PageVerify p ON d.oPageVerify = p.PageVerify INNER JOIN
                      dbo.d_UserAccess a ON d.oUserAccess = a.UserAccess INNER JOIN
                      dbo.mgr_run r ON d.run_id = r.run_id
WHERE     (d.srv_id = @srv_id) AND (d.inst_id = @inst_id) AND (d.dbid = @dbId)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>= Parameters!srv_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inst_id">
            <Value>= Parameters!inst_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dbId">
            <Value>= Parameters!dbId.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
