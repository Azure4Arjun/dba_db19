        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="prev_reftime">
          <DataField>prev_reftime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="reftime">
          <DataField>reftime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="type">
          <DataField>type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="isMsShipped">
          <DataField>isMsShipped</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="isPublished">
          <DataField>isPublished</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="qObjects">
          <DataField>qObjects</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="qCREATEd">
          <DataField>qCREATEd</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="qModified">
          <DataField>qModified</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     d.name, t.prev_reftime, t.reftime, y.name AS type, o.isMsShipped, o.isPublished, o.qObjects, o.qCREATEd, o.qModified
FROM         dbo.c_database d INNER JOIN
                      dbo.c_dbobjects o ON d.run_id = o.run_id AND d.srv_id = o.srv_id AND d.inst_id = o.inst_id AND d.dbid = o.dbid INNER JOIN
                      dbo.mgr_dbobject_reftime t ON t.srv_id = d.srv_id AND t.inst_id = d.inst_id AND t.dbId = d.dbid AND t.run_id = d.run_id INNER JOIN
                      dbo.d_DbObjectType y ON o.type = y.type
WHERE     (d.srv_id = @srv_id) AND (d.inst_id = @inst_id)
ORDER BY d.dbid, o.type</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>=Parameters!srv_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inst_id">
            <Value>=Parameters!inst_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="InstWide">
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Certificates">
          <DataField>Certificates</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Cursors">
          <DataField>Cursors</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Endpoints">
          <DataField>Endpoints</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="iMail">
          <DataField>iMail</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Linked_Servers">
          <DataField>Linked Servers</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Trace">
          <DataField>Trace</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     i.name,
                          (SELECT     SUM(qObjects)
                            FROM          c_certificates a
                            WHERE      i.srv_id = a.srv_id AND i.inst_id = a.inst_id) 'Certificates',
                          (SELECT     SUM(qObjects)
                            FROM          c_cursors a
                            WHERE      i.srv_id = a.srv_id AND i.inst_id = a.inst_id) 'Cursors',
                          (SELECT     SUM(qObjects)
                            FROM          c_endpoints a
                            WHERE      i.srv_id = a.srv_id AND i.inst_id = a.inst_id) 'Endpoints',
                          (SELECT     COUNT(*)
                            FROM          c_imail a
                            WHERE      i.srv_id = a.srv_id AND i.inst_id = a.inst_id AND a.hostId &gt; 0) 'iMail',
                          (SELECT     SUM(qObjects)
                            FROM          c_linked_servers a
                            WHERE      i.srv_id = a.srv_id AND i.inst_id = a.inst_id) 'Linked Servers',
                          (SELECT     COUNT(DISTINCT trace_id)
                            FROM          c_trace a
                            WHERE      i.srv_id = a.srv_id AND i.inst_id = a.inst_id) 'Trace'
FROM         mgr_instance i
WHERE     i.srv_id = @srv_id AND i.inst_id = @inst_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>=Parameters!srv_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inst_id">
            <Value>=Parameters!inst_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
  </DataSets>
