        <CommandText>SELECT     m.inst_id, m.name
FROM         dbo.mgr_instance m INNER JOIN
                      dbo.c_instance i ON m.srv_id = i.srv_id AND m.inst_id = i.inst_id AND i.major = 9
WHERE     (m.srv_id = @srv_id)</CommandText>
        <QueryParameters>
