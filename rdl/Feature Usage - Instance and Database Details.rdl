        </Field>
        <Field Name="oReadCommittedSnapshotOn">
          <DataField>oReadCommittedSnapshotOn</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="qIndexedViews">
          <DataField>qIndexedViews</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Mirroring">
          <DataField>Mirroring</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="IsRosetta">
          <DataField>IsRosetta</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="Backups">
          <DataField>Backups</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Notifications">
          <DataField>Notifications</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NotificationSubscriptions">
          <DataField>NotificationSubscriptions</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FTTables">
          <DataField>FTTables</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="LastCrawl">
          <DataField>LastCrawl</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PartitionSchemes">
          <DataField>PartitionSchemes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ReportsGenerated">
          <DataField>ReportsGenerated</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Queues">
          <DataField>Queues</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TextInRow">
          <DataField>TextInRow</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     r.run_id, r.datein, d.dbid, d.name, t.prev_reftime, t.reftime, CASE WHEN (oPublished = 1 OR
                      oSubscribed = 1 OR
                      oMergePublished = 1 OR
                      oMergeSubscribed = 1 OR
                      oDistributor = 1) THEN 1 ELSE 0 END AS [Replication], d.oFullTextEnabled, d2.oBrokerEnabled, d2.oReadCommittedSnapshotOn, d2.qIndexedViews, 
                      d2.mirroringWitness AS Mirroring, d2.IsRosetta,
                          (SELECT     SUM(qObjects)
                            FROM          c_backups a
                            WHERE      a.run_id = d.run_id AND a.srv_id = d.srv_id AND a.inst_id = d.inst_id AND a.dbid = d.dbid) AS Backups,
                          (SELECT     SUM(qObjects)
                            FROM          c_event_notifications a
                            WHERE      a.run_id = d.run_id AND a.srv_id = d.srv_id AND a.inst_id = d.inst_id AND a.dbid = d.dbid) AS Notifications,
                          (SELECT     SUM(qObjects)
                            FROM          c_notification_subscriptions a
                            WHERE      a.run_id = d.run_id AND a.srv_id = d.srv_id AND a.inst_id = d.inst_id AND a.dbid = d.dbid) AS NotificationSubscriptions,
                          (SELECT     SUM(TABLEs)
                            FROM          c_ft a
                            WHERE      a.run_id = d.run_id AND a.srv_id = d.srv_id AND a.inst_id = d.inst_id AND a.dbid = d.dbid) AS FTTables,
                          (SELECT     CASE WHEN LastCrawl &gt; 0 THEN LastCrawl ELSE NULL END
                            FROM          c_ft a
                            WHERE      a.run_id = d.run_id AND a.srv_id = d.srv_id AND a.inst_id = d.inst_id AND a.dbid = d.dbid) AS LastCrawl,
                          (SELECT     COUNT(*)
                            FROM          c_partition_scheme a
                            WHERE      a.run_id = d.run_id AND a.srv_id = d.srv_id AND a.inst_id = d.inst_id AND a.dbid = d.dbid) AS PartitionSchemes,
                          (SELECT     SUM(qObjects)
                            FROM          c_rs a
                            WHERE      a.run_id = d.run_id AND a.srv_id = d.srv_id AND a.inst_id = d.inst_id AND a.dbid = d.dbid) AS ReportsGenerated,
                          (SELECT     SUM(Queues)
                            FROM          c_sb a
                            WHERE      a.run_id = d.run_id AND a.srv_id = d.srv_id AND a.inst_id = d.inst_id AND a.dbid = d.dbid) AS Queues,
                          (SELECT     SUM(qObjects)
                            FROM          c_TableTextInRowLimit a
                            WHERE      a.run_id = d.run_id AND a.srv_id = d.srv_id AND a.inst_id = d.inst_id AND a.dbid = d.dbid AND Limit &gt; 0) AS TextInRow
FROM         dbo.c_database d INNER JOIN
                      dbo.c_database2 d2 ON d.run_id = d2.run_id AND d.srv_id = d2.srv_id AND d.inst_id = d2.inst_id AND d.dbid = d2.dbid INNER JOIN
                      dbo.mgr_dbobject_reftime t ON d.run_id = t.run_id AND d.srv_id = t.srv_id AND d.inst_id = t.inst_id AND d.dbid = t.dbId INNER JOIN
                      dbo.mgr_run r ON d.run_id = r.run_id
WHERE     (d.srv_id = @srv_id) AND (d.inst_id = @inst_id)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>=Parameters!srv_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inst_id">
            <Value>=Parameters!inst_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
