          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IntegratedOnly">
          <DataField>IntegratedOnly</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SingleUser">
          <DataField>SingleUser</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NumLicenses">
          <DataField>NumLicenses</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AD">
          <DataField>AD</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     i.Collation, CASE i.IsClustered WHEN 1 THEN 'Yes' ELSE 'No' END AS 'Clustered', CASE i.IsFullTextInstalled WHEN 1 THEN 'Yes' ELSE 'No' END AS FT,
                       CASE i.IsIntegratedSecurityOnly WHEN 1 THEN 'Yes' ELSE 'No' END AS IntegratedOnly, 
                      CASE i.IsSingleUser WHEN 1 THEN 'Yes' ELSE 'No' END AS SingleUser, l.name, i.NumLicenses, 
                      CASE i.IsAD WHEN 1 THEN 'Yes' ELSE 'No' END AS AD
FROM         dbo.c_instance2 i INNER JOIN
                      dbo.d_LicenseType l ON i.LicenseType = l.LicenseType
WHERE     (i.srv_id = @srv_id) AND (i.inst_id = @inst_id)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>= Parameters!srv_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inst_id">
            <Value>= Parameters!inst_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="TraceFlags">
      <Fields>
        <Field Name="flags">
          <DataField>flags</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     flags
FROM         c_GlobalTraceFlags
WHERE     srv_id = @srv_id AND inst_id = @inst_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>= Parameters!srv_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inst_id">
            <Value>= Parameters!inst_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Configs">
      <Fields>
        <Field Name="comment">
          <DataField>comment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Default">
          <DataField>Default</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Current">
          <DataField>Current</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     d.comment, d.value 'Default', c.value 'Current'
FROM         c_syscurconfigs c JOIN
                      c_instance i ON (c.srv_id = i.srv_id AND c.inst_id = i.inst_id) JOIN
                      d_syscurconfigs d ON (c.config = d.config AND d.ver_id = i.major - 7)
WHERE     c.srv_id = @srv_id AND c.inst_id = @inst_id AND c.config &gt; 4</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>= Parameters!srv_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inst_id">
            <Value>= Parameters!inst_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Errorlog">
      <Fields>
        <Field Name="path">
          <DataField>path</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="logsize">
          <DataField>logsize</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="depth">
          <DataField>depth</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     path, logsize, depth
FROM         c_errorlog c
WHERE     c.srv_id = @srv_id AND c.inst_id = @inst_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>= Parameters!srv_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inst_id">
            <Value>= Parameters!inst_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
  </DataSets>
