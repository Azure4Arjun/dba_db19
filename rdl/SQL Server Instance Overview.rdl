  <Code />
  <Width>9.20833in</Width>
  <DataSets>
    <DataSet Name="Servers">
      <Fields>
        <Field Name="srv_id">
          <DataField>srv_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     srv_id, name
FROM         dbo.mgr_server s
WHERE     EXISTS
                          (SELECT     *
                            FROM          mgr_instance i
                            WHERE      s.srv_id = i.srv_id)
ORDER BY name</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="Instances">
      <Fields>
        <Field Name="inst_id">
          <DataField>inst_id</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     inst_id, name
FROM         dbo.mgr_instance
WHERE     (srv_id = @srv_id)
ORDER BY inst_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>= Parameters!srv_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="InstInfo">
      <Fields>
        <Field Name="major">
          <DataField>major</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="version">
          <DataField>version</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="edition">
          <DataField>edition</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="plevel">
          <DataField>plevel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="State">
          <DataField>State</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="run_id">
          <DataField>run_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="datein">
          <DataField>datein</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     i.major, i.version, i.edition, i.plevel, i.IsRunning AS State, r.run_id, r.datein
FROM         dbo.c_instance i INNER JOIN
                      dbo.mgr_run r ON i.run_id = r.run_id
WHERE     (i.srv_id = @srv_id) AND (i.inst_id = @inst_id)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>= Parameters!srv_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inst_id">
            <Value>= Parameters!inst_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Traces">
      <Fields>
        <Field Name="trace_id">
          <DataField>trace_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="value">
          <DataField>value</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     t.trace_id, p.name, t.value
FROM         c_trace t JOIN
                      d_TraceProperty p ON (t.property = p.property)
WHERE     srv_id = @srv_id AND inst_id = @inst_id
ORDER BY t.property</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>= Parameters!srv_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inst_id">
            <Value>= Parameters!inst_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="InstInfo2">
      <Fields>
        <Field Name="Collation">
          <DataField>Collation</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Clustered">
          <DataField>Clustered</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FT">
