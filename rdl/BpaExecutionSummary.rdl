  <DataSets>
    <DataSet Name="sqlbpa">
      <Fields>
        <Field Name="sqlserver">
          <DataField>sqlserver</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Complied">
          <DataField>Complied</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Partial">
          <DataField>Partial</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Not_Complied">
          <DataField>Not Complied</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Exception">
          <DataField>Exception</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>sqlbpa</DataSourceName>
        <CommandText>SELECT     sqlserver, SUM(CASE WHEN bir.result = 0 THEN 1 ELSE 0 END) AS Complied, SUM(CASE WHEN bir.result = 1 THEN 1 ELSE 0 END) AS Partial, 
                      SUM(CASE WHEN bir.result = 2 THEN 1 ELSE 0 END) AS [Not Complied], SUM(CASE WHEN bir.result = 3 THEN 1 ELSE 0 END) AS Exception
FROM         bpa_instance_results bir
WHERE     (execution_id = @executionid)
GROUP BY sqlserver</CommandText>
        <QueryParameters>
          <QueryParameter Name="@executionid">
            <Value>=Parameters!executionid.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="result_summary">
      <Fields>
        <Field Name="sqlserver">
          <DataField>sqlserver</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="category">
          <DataField>category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="bestpractice_name">
          <DataField>bestpractice_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Result">
          <DataField>Result</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="message">
          <DataField>message</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>sqlbpa</DataSourceName>
        <CommandText>select 	bir.sqlserver,
	bb.category, 
	bb.bestpractice_name, 
	case when bir.result=0 then 'Complied' when bir.result=1 then 'Partial Compliance' when bir.result=2 then 'Non Compliance' when bir.result=3 then 'Error' end Result,
	bir.message
from dbo.bpa_instance_results bir join bpa_bestpractices bb on (bir.bestpractice_id=bb.bestpractice_id)
where execution_id=@executionid
order by result</CommandText>
        <QueryParameters>
          <QueryParameter Name="@executionid">
            <Value>=Parameters!executionid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="summaryinfo">
      <Fields>
        <Field Name="exec_time">
          <DataField>exec_time</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="job_name">
          <DataField>job_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>sqlbpa</DataSourceName>
        <CommandText>select bp.exec_time, bp.job_name
from bpa_job_execution_logs bp
where bp.execution_id=@executionid</CommandText>
        <QueryParameters>
          <QueryParameter Name="@executionid">
            <Value>=Parameters!executionid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
