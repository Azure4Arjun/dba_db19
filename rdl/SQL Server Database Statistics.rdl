  <DataSources>
    <DataSource Name="SQLH2Repository">
      <rd:DataSourceID>e2c97ad0-3c49-4b40-973e-0718efb236cb</rd:DataSourceID>
      <DataSourceReference>SQLH2Repository</DataSourceReference>
