  <Code />
  <Width>14.375in</Width>
  <DataSets>
    <DataSet Name="Servers">
      <Fields>
        <Field Name="srv_id">
          <DataField>srv_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     srv_id, name
FROM         dbo.mgr_server s
WHERE     EXISTS
                          (SELECT     *
                            FROM          mgr_instance i
                            WHERE      s.srv_id = i.srv_id)
ORDER BY name</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="Instances">
      <Fields>
        <Field Name="inst_id">
          <DataField>inst_id</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     inst_id, name
FROM         dbo.mgr_instance
WHERE     (srv_id = @srv_id)
ORDER BY inst_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>= Parameters!srv_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Databases">
      <Fields>
        <Field Name="dbid">
          <DataField>dbid</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CompatibilityLevel">
          <DataField>CompatibilityLevel</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="CreateDate">
          <DataField>CreateDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="dbSize">
          <DataField>dbSize</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="SpaceAvailable">
          <DataField>SpaceAvailable</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="txSize">
          <DataField>txSize</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="txSpaceAvailable">
          <DataField>txSpaceAvailable</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="txLastBackup">
          <DataField>txLastBackup</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="RecoveryModel">
          <DataField>RecoveryModel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="fgid">
          <DataField>fgid</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="FileGroupName">
          <DataField>FileGroupName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="fgSize">
          <DataField>fgSize</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="fid">
          <DataField>fid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FileName">
          <DataField>FileName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="fSize">
          <DataField>fSize</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="fSpaceUsed">
          <DataField>fSpaceUsed</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="AutogrowMax">
          <DataField>AutogrowMax</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Growth">
          <DataField>Growth</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     d.dbid, d.name, d.CompatibilityLevel, d.CreateDate, d.dbSize, d.SpaceAvailable, d.txSize, d.txSpaceAvailable, d.txLastBackup, 
                      r.name AS RecoveryModel, g.fgid, g.name AS FileGroupName, g.fgSize, f.fid, f.name AS FileName, f.fSize, f.fSpaceUsed, 
                      CASE Growth WHEN 0 THEN 'No Autogrow' ELSE (CASE MaxSize WHEN - 1 THEN 'Unlimited' ELSE LTRIM(STR(MaxSize)) END) END AS AutogrowMax, 
                      CASE WHEN Growth &gt; 0 AND GrowthType = 0 THEN LTRIM(STR(Growth)) + 'MB' WHEN Growth &gt; 0 AND GrowthType = 1 THEN LTRIM(STR(Growth)) 
                      + '%' END AS Growth
FROM         dbo.c_database d INNER JOIN
                      dbo.c_filegroup g ON d.srv_id = g.srv_id AND d.inst_id = g.inst_id AND d.dbid = g.dbid INNER JOIN
                      dbo.c_dbfile f ON f.srv_id = g.srv_id AND f.inst_id = g.inst_id AND f.dbid = g.dbid AND (f.fgid = g.fgid OR
                      f.fgid = 0) INNER JOIN
                      dbo.d_RecoveryModel r ON d.oRecoveryModel = r.RecoveryModel
WHERE     (d.srv_id = @srv_id) AND (d.inst_id = @inst_id)
ORDER BY d.srv_id, d.inst_id, d.dbid, g.fgid, f.fid</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>= Parameters!srv_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inst_id">
            <Value>= Parameters!inst_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="RunInfo">
      <Fields>
        <Field Name="run_id">
          <DataField>run_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="datein">
          <DataField>datein</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     TOP 1 r.run_id, r.datein
FROM         dbo.mgr_run r INNER JOIN
                      dbo.c_database d ON r.run_id = d.run_id
WHERE     (d.srv_id = @srv_id) AND (d.inst_id = @inst_id)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>=Parameters!srv_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inst_id">
            <Value>=Parameters!inst_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
  </DataSets>
