  <Code />
  <Width>11in</Width>
  <DataSets>
    <DataSet Name="Servers">
      <Fields>
        <Field Name="srv_id">
          <DataField>srv_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     srv_id, name
FROM         dbo.mgr_server s
WHERE     EXISTS
                          (SELECT     *
                            FROM          perf p
                            WHERE      s.srv_id = p.srv_id)
ORDER BY name</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="Counters">
      <Fields>
        <Field Name="counter_id">
          <DataField>counter_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="counter">
          <DataField>counter</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     counter_id, object + ' | ' + counter + CASE WHEN instance IS NULL THEN '' ELSE ' | ' + instance END AS counter
FROM         dbo.dp_counters d
WHERE     EXISTS
                          (SELECT     *
                            FROM          perf p
                            WHERE      p.srv_id = @srv_id AND d.counter_id = p.counter_id)
ORDER BY object, counter, instance</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>=Parameters!srv_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Perf">
      <Fields>
        <Field Name="stamp">
          <DataField>stamp</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="value">
          <DataField>value</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLH2Repository</DataSourceName>
        <CommandText>SELECT     stamp, value
FROM         perf p
WHERE     srv_id = @srv_id AND counter_id = @counter_id AND stamp BETWEEN @a_start AND @a_finish
ORDER BY stamp</CommandText>
        <QueryParameters>
          <QueryParameter Name="@srv_id">
            <Value>=Parameters!srv_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@counter_id">
            <Value>=Parameters!counter_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@a_start">
            <Value>=Parameters!a_start.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@a_finish">
            <Value>=Parameters!a_finish.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
  </DataSets>
